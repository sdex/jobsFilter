{
    "name": "Upwork Helper",
    "version": "1.0.1",
    "description": "Upwork Helper",
    "homepage_url": "https://sdex.dev",
    "author": "Yuriy",
    "manifest_version": 2,
    "permissions":
    [
        "<all_urls>",
        "tabs"
    ],
    "browser_action":
    {
        "default_title": "Upwork Helper",
        "default_popup": "src/popup/popup.html",
        "default_icon":
        {
            "16": "assets/icon.16.jpg",
            "48": "assets/icon.48.jpg",
            "128": "assets/icon.128.jpg"
        }
    },
    "content_scripts":
    [
        {
            "run_at": "document_end",
            "matches":
            [
                "https://www.upwork.com/ab/jobs/search/*",
                "https://www.upwork.com/ab/find-work/*"
            ],
            "js":
            [
                "src/script.js"
            ]
        }
    ],
    "icons":
    {
        "16": "assets/icon.16.jpg",
        "48": "assets/icon.48.jpg",
        "128": "assets/icon.128.jpg"
    },
    "browser_specific_settings":
    {
        "gecko":
        {
            "id": "lioce@protonmail.com"
        }
    }
}